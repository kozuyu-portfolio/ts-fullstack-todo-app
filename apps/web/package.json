{
    "name": "@ts-fullstack-todo/web",
    "private": "false",
    "version": "0.1.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "env:dev": "cross-env VITE_STAGE=dev",
        "env:stg": "cross-env VITE_STAGE=stg",
        "env:prod": "cross-env VITE_STAGE=prod",
        "build:dev": "pnpm env:dev tsc --project ./tsconfig.build.json && pnpm env:dev vite build",
        "build:stg": "pnpm env:stg tsc --project ./tsconfig.build.json && pnpm env:stg vite build",
        "build:prod": "pnpm env:prod tsc --project ./tsconfig.build.json && pnpm env:prod vite build",
        "preview": "vite preview",
        "test": "echo \"unit test is not implemented\"",
        "dev:api": "pnpm --filter @ts-fullstack-todo/api start:dev",
        "dev:web": "vite --host",
        "e2e:run": "cypress run --headless --config-file cypress.config.mjs",
        "test:e2e": "start-server-and-test 'pnpm dev:api' http://localhost:3000 'start-server-and-test \"pnpm dev:web\" http://localhost:5173 e2e:run'"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.0",
        "@hey-api/client-axios": "^0.7.1",
        "@hookform/resolvers": "^5.0.1",
        "@mui/icons-material": "^7.1.0",
        "@mui/material": "^7.1.0",
        "@tanstack/react-query": "^5.76.1",
        "@tanstack/react-router": "^1.120.3",
        "@tanstack/react-router-devtools": "^1.120.3",
        "dayjs": "^1.11.13",
        "jotai": "^2.12.4",
        "jotai-tanstack-query": "^0.9.0",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-dropzone": "^14.3.8",
        "react-hook-form": "^7.56.3",
        "zod": "^3.24.4"
    },
    "devDependencies": {
        "@eslint/js": "^9.25.0",
        "@tanstack/router-plugin": "^1.120.3",
        "@types/react": "^19.1.2",
        "@types/react-dom": "^19.1.2",
        "@vitejs/plugin-react": "^4.4.1",
        "cypress": "^14.3.3",
        "eslint": "^9.25.0",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-react-refresh": "^0.4.19",
        "globals": "^16.0.0",
        "start-server-and-test": "^2.0.12",
        "typescript": "~5.8.3",
        "typescript-eslint": "^8.30.1",
        "vite": "^6.3.5",
        "wait-on": "^8.0.3"
    }
}
